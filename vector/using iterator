
#include <iostream>
#include<bits/stdc++.h>
using namespace std;

int main()
{
vector<int>v={1,2,3,4,5};
for(int i=0;i<v.size();i++)
{
    cout<<v[i]<<"\t"<<endl;
} 
                           //Method 2

//by using iterators--they are pointer like structure that points to the value of element in a container!
vector<int>::iterator it=v.begin();  //declaration
for(it=v.begin();it!=v.end();++it){
    cout<< *it ;                         //dereferencing
}

    return 0;
}
